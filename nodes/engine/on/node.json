  "phrases": {
    "active": "On"
  },
  "require": {
    "famo.us/core/Engine": "0.x.x"
  },
  "ports": {
    "input": {
      "type": {
        "type": "string",
        "title": "Type",
        "description": "event type key (for example, 'click')"
      },
      "handler": {
        "type": "function",
        "title": "Handler",
        "description": "handler callback TODO: not sure what the function receives",
        "args": ["type","obj"]
      }
    },
    "output": {
      "event_handler": {
        "type": "EventHandler",
        "title": "Event handler"
      }
    }
  }
}
